{
    "name": "namelesscoder/patternlab-fluid-patternengine",
    "type": "patternlab-patternengine",
    "license": "MIT",
    "require": {
        "pattern-lab/core": "^2.8",
        "typo3fluid/fluid": "^2.4"
    },
    "extra": {
        "patternlab": {
            "config": {
                "patternExtension": "fluid",
                "lineageMatch": "([<{]f:render.*partial[=:\\s\"']*([^\"']+)[\"']*)",
                "lineageMatchKey": 2
            }
        }
    },
    "autoload": {
        "psr-4": {
            "NamelessCoder\\FluidPatternEngine\\": "src/"
        }
    }
}
